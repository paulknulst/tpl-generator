{
  "name": "third-party-license-generator",
  "version": "1",
  "description": "this script will generate the third party license as a html file based on the packages from your application. Only dependencies, not dev-dependencies",
  "main": "dist/index.js",
  "scripts": {
    "build": "yarn clean:dist && babel src --out-dir dist",
    "clean:dist": "rimraf ./dist"
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-jest": "^22.0.4",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1"
  },
  "babel": {
    "env": {
      "production": {
        "presets": [
          [
            "es2015",
            {
              "modules": false
            }
          ],
          "stage-2"
        ],
        "ignore": [
          "__tests__"
        ],
        "plugins": [
          "transform-es2015-modules-commonjs"
        ]
      },
      "development": {
        "presets": [
          [
            "es2015",
            {
              "modules": false
            }
          ],
          "stage-2"
        ],
        "ignore": [
          "__tests__"
        ]
      },
      "test": {
        "presets": [
          "es2015",
          "stage-2"
        ]
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/paulknulst/tpl-generator.git"
  },
  "keywords": [
    "license",
    "third party",
    "auto generation"
  ],
  "author": "Paul Knulst",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/paulknulst/tpl-generator/issues"
  },
  "homepage": "https://github.com/paulknulst/tpl-generator#readme",
  "devDependencies": {
    "rimraf": "^3.0.2"
  }
}
